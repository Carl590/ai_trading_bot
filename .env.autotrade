# Auto-Trading Configuration Template
# Copy this to your .env file and configure your settings

# ==============================================
# AUTO-TRADING WALLET CONFIGURATION
# ==============================================

# Private key for auto-trading wallet (hex format without 0x prefix)
# ⚠️ SECURITY: Keep this secret and use a dedicated wallet with limited funds
AUTO_TRADE_WALLET_PRIVATE_KEY=your_wallet_private_key_here

# Default amount to buy per auto-trade (in SOL)
AUTO_TRADE_AMOUNT_SOL=0.1

# Maximum daily auto-trade amount (safety limit)
AUTO_TRADE_DAILY_LIMIT_SOL=1.0

# Minimum confidence score to trigger auto-trade (0.0-1.0)
AUTO_TRADE_MIN_CONFIDENCE=0.8

# Maximum slippage for auto-trades (basis points, 300 = 3%)
AUTO_TRADE_MAX_SLIPPAGE_BPS=300

# ==============================================
# AUTO-TRADING SAFETY SETTINGS
# ==============================================

# Enable/disable auto-trading globally
AUTO_TRADE_ENABLED=false

# Whitelist of tokens that can be auto-traded (comma separated)
# Leave empty to allow all high-confidence tokens
AUTO_TRADE_WHITELIST=

# Blacklist of tokens to never auto-trade (comma separated)
AUTO_TRADE_BLACKLIST=

# Maximum number of auto-trades per hour
AUTO_TRADE_MAX_PER_HOUR=5

# ==============================================
# NOTIFICATION SETTINGS
# ==============================================

# Telegram chat ID for auto-trade notifications
AUTO_TRADE_NOTIFICATION_CHAT_ID=

# Discord webhook for auto-trade notifications
AUTO_TRADE_DISCORD_WEBHOOK=

# Email notifications for auto-trades
AUTO_TRADE_EMAIL_NOTIFICATIONS=false
AUTO_TRADE_EMAIL_TO=

# ==============================================
# ADVANCED SETTINGS
# ==============================================

# Use priority fees for faster execution
AUTO_TRADE_USE_PRIORITY_FEES=true

# Priority fee amount (lamports)
AUTO_TRADE_PRIORITY_FEE=10000

# MEV protection settings
AUTO_TRADE_MEV_PROTECTION=true

# Maximum gas price (lamports per compute unit)
AUTO_TRADE_MAX_GAS_PRICE=50000